    "ajv": "^8.17.1",
    "fastify": "^5.3.2",
    "speakeasy": "^2.0.0",
    "ts-node": "^10.9.2",
    "undici": "^6.21.3",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/speakeasy": "^2.0.10",
